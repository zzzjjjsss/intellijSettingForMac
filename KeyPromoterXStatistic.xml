<application>
  <component name="KeyPromoterXStatistic">
    <option name="statistics">
      <Statistic Action="Close thumbnails">
        <Item>
          <IdeaActionID>Images.Thumbnails.Hide</IdeaActionID>
          <ShortCut>'⌘W'</ShortCut>
          <Description>Close thumbnails</Description>
          <Count>1</Count>
        </Item>
      </Statistic>
      <Statistic Action="Collapse All">
        <Item>
          <IdeaActionID>Maven.CollapseAll</IdeaActionID>
          <ShortCut>'⌘-' or '⌘-'</ShortCut>
          <Description>Collapse All</Description>
          <Count>2</Count>
        </Item>
      </Statistic>
      <Statistic Action="Copy">
        <Item>
          <IdeaActionID>$Copy</IdeaActionID>
          <ShortCut>'⌘C'</ShortCut>
          <Description>Copy</Description>
          <Count>1</Count>
        </Item>
      </Statistic>
      <Statistic Action="Declaration">
        <Item>
          <IdeaActionID>GotoDeclaration</IdeaActionID>
          <ShortCut>'⌃G'</ShortCut>
          <Description>Declaration</Description>
          <Count>2</Count>
        </Item>
      </Statistic>
      <Statistic Action="Exclude">
        <Item>
          <IdeaActionID>UsageView.Exclude</IdeaActionID>
          <ShortCut>'⌦' or '⌫' or '⌘⌫'</ShortCut>
          <Description>Exclude</Description>
          <Count>1</Count>
        </Item>
      </Statistic>
      <Statistic Action="Find">
        <Item>
          <IdeaActionID>ActivateFindToolWindow</IdeaActionID>
          <ShortCut>'⌘7'</ShortCut>
          <Description>Find</Description>
          <Count>1</Count>
        </Item>
      </Statistic>
      <Statistic Action="Hide All Windows">
        <Item>
          <IdeaActionID>HideAllWindows</IdeaActionID>
          <ShortCut>'⇧⌘F12'</ShortCut>
          <Description>Hide All Windows</Description>
          <Count>2</Count>
        </Item>
      </Statistic>
      <Statistic Action="Jump to Source">
        <Item>
          <IdeaActionID>EditSource</IdeaActionID>
          <ShortCut>'⌘↓' or 'F4'</ShortCut>
          <Description>Jump to Source</Description>
          <Count>6</Count>
        </Item>
      </Statistic>
      <Statistic Action="Project">
        <Item>
          <IdeaActionID>ActivateProjectToolWindow</IdeaActionID>
          <ShortCut>'⌘1'</ShortCut>
          <Description>Project</Description>
          <Count>1</Count>
        </Item>
      </Statistic>
      <Statistic Action="Refresh">
        <Item>
          <IdeaActionID>Vcs.Log.Refresh</IdeaActionID>
          <ShortCut>'⌘R'</ShortCut>
          <Description>Refresh</Description>
          <Count>10</Count>
        </Item>
      </Statistic>
      <Statistic Action="Rerun 'Application'">
        <Item>
          <IdeaActionID>Rerun</IdeaActionID>
          <ShortCut>'⌘R'</ShortCut>
          <Description>Rerun 'Application'</Description>
          <Count>1</Count>
        </Item>
      </Statistic>
      <Statistic Action="Restore Windows">
        <Item>
          <IdeaActionID>HideAllWindows</IdeaActionID>
          <ShortCut>'⇧⌘F12'</ShortCut>
          <Description>Restore Windows</Description>
          <Count>2</Count>
        </Item>
      </Statistic>
      <Statistic Action="Revert...">
        <Item>
          <IdeaActionID>ChangesView.Revert</IdeaActionID>
          <ShortCut>'⌥⌘Z'</ShortCut>
          <Description>Revert...</Description>
          <Count>5</Count>
        </Item>
      </Statistic>
      <Statistic Action="Run">
        <Item>
          <IdeaActionID>ActivateRunToolWindow</IdeaActionID>
          <ShortCut>'⌘4'</ShortCut>
          <Description>Run</Description>
          <Count>2</Count>
        </Item>
      </Statistic>
      <Statistic Action="Show Image Thumbnails">
        <Item>
          <IdeaActionID>Images.ShowThumbnails</IdeaActionID>
          <ShortCut>'⇧⌘T'</ShortCut>
          <Description>Show Image Thumbnails</Description>
          <Count>1</Count>
        </Item>
      </Statistic>
      <Statistic Action="Stop 'ActiveMqTest'">
        <Item>
          <IdeaActionID>Stop</IdeaActionID>
          <ShortCut>'⌘F2'</ShortCut>
          <Description>Stop 'ActiveMqTest'</Description>
          <Count>2</Count>
        </Item>
      </Statistic>
      <Statistic Action="Stop 'ApiGatewayApplication'">
        <Item>
          <IdeaActionID>Stop</IdeaActionID>
          <ShortCut>'⌘F2'</ShortCut>
          <Description>Stop 'ApiGatewayApplication'</Description>
          <Count>1</Count>
        </Item>
      </Statistic>
      <Statistic Action="Stop 'Application'">
        <Item>
          <IdeaActionID>Stop</IdeaActionID>
          <ShortCut>'⌘F2'</ShortCut>
          <Description>Stop 'Application'</Description>
          <Count>5</Count>
        </Item>
      </Statistic>
      <Statistic Action="Stop 'DateTimeUtil'">
        <Item>
          <IdeaActionID>Stop</IdeaActionID>
          <ShortCut>'⌘F2'</ShortCut>
          <Description>Stop 'DateTimeUtil'</Description>
          <Count>2</Count>
        </Item>
      </Statistic>
      <Statistic Action="Stop 'Test'">
        <Item>
          <IdeaActionID>Stop</IdeaActionID>
          <ShortCut>'⌘F2'</ShortCut>
          <Description>Stop 'Test'</Description>
          <Count>1</Count>
        </Item>
      </Statistic>
      <Statistic Action="TODO">
        <Item>
          <IdeaActionID>ActivateTODOToolWindow</IdeaActionID>
          <ShortCut>'⌘6'</ShortCut>
          <Description>TODO</Description>
          <Count>1</Count>
        </Item>
      </Statistic>
    </option>
  </component>
</application>